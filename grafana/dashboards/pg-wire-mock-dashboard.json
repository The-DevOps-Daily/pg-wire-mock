            "expr": "histogram_quantile(0.50, rate(pgwire_query_duration_seconds_bucket[5m]))",
            "legendFormat": "p50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.90, rate(pgwire_query_duration_seconds_bucket[5m]))",
            "legendFormat": "p90",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.95, rate(pgwire_query_duration_seconds_bucket[5m]))",
            "legendFormat": "p95",
            "refId": "C"
          },
          {
            "expr": "histogram_quantile(0.99, rate(pgwire_query_duration_seconds_bucket[5m]))",
